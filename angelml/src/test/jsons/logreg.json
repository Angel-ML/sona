{
  "data": {
    "format": "libsvm",
    "indexrange": 123,
    "validateratio": 0.1
  },
  "train": {
    "epoch": 10,
    "lr": 1.0
  },
  "model": {
    "modeltype": "T_FLOAT_SPARSE_LONGKEY"
  },
  "default_optimizer": {
    "type": "Momentum",
    "reg2": 0.01
  },
  "layers": [
    {
      "name": "input",
      "type": "simpleinputlayer",
      "outputdim": 1,
      "transfunc": "identity"
    },
    {
      "name": "simplelosslayer",
      "type": "losslayer",
      "lossfunc": "logloss",
      "inputlayer": "input"
    }
  ]
}
